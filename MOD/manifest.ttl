@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_overdriver_#_overdriver_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxoverdriver.html> ;
        modgui:stylesheet <modgui/stylesheet-gxoverdriver.css> ;
        modgui:screenshot <modgui/screenshot-gxoverdriver.png> ;
        modgui:thumbnail <modgui/thumbnail-gxoverdriver.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxOverdriver" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "wood4" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DRIVE" ;
            lv2:name "DRIVE" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "BASS" ;
            lv2:name "BASS" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "TREBLE" ;
            lv2:name "TREBLE" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "VOLUME" ;
            lv2:name "VOLUME" ;
        ] ;
    ] .